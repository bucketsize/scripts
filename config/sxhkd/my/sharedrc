// launcher
super + d
	~/.luarocks/bin/mxctl.control fun dmenu_run

// lockscreen
control + alt + l
	~/.luarocks/bin/mxctl.control cmd scr_lock

// screenshot
Print
	~/.luarocks/bin/mxctl.control cmd scr_cap

control + Print
	~/.luarocks/bin/mxctl.control cmd scr_cap_sel

control + alt + Delete
	~/.luarocks/bin/mxctl.control fun dmenu_exit

// volume
control + bracketright
	~/.luarocks/bin/mxctl.control cmd vol_up

control + bracketleft
	~/.luarocks/bin/mxctl.control cmd vol_down

control + backslash
	~/.luarocks/bin/mxctl.control cmd vol_mute

@XF86AudioRaiseVolume
	~/.luarocks/bin/mxctl.control cmd vol_up

@XF86AudioLowerVolume
	~/.luarocks/bin/mxctl.control cmd vol_down

@XF86AudioMute
	~/.luarocks/bin/mxctl.control cmd vol_mute

control + alt + 2
	~/.luarocks/bin/mxctl.control fun dmenu_setup_video

control + alt + 3
	~/.luarocks/bin/mxctl.control fun dmenu_select_pa_sinks

control + alt + 4
	~/.luarocks/bin/mxctl.control cmd kb_led_on

control + alt + 5
	~/.luarocks/bin/mxctl.control cmd kb_led_off
